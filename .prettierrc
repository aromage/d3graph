{
  "singleQuote": true,
  "parser": "flow",
  "semi": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "quoteProps": "as-needed",
  "vueIndentScriptAndStyle": false,
  "printWidth": 120,
  "htmlWhitespaceSensitivity": "css",
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "parser": "vue"
      }
    }
  ]
}
